{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/",
      "dest": "HATWIN.html"
    },
    {
      "src": "/(LAY.jpg|.*\.jpg|.*\.png|.*\.gif|.*\.svg)",
      "dest": "public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "$1"
    }
  ]
}